                   rdfs:subClassOf :Ingrediente .


###  http://example.org/polvo-filosofico#Maquina
:Maquina rdf:type owl:Class ;
         rdfs:subClassOf :Agente .


###  http://example.org/polvo-filosofico#Pedido
:Pedido rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :contemPrato ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Prato
                        ] .


###  http://example.org/polvo-filosofico#Pessoa
:Pessoa rdf:type owl:Class ;
        rdfs:subClassOf :Agente .


###  http://example.org/polvo-filosofico#Polvo
:Polvo rdf:type owl:Class ;
       rdfs:subClassOf :Animal ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :come ;
                         owl:allValuesFrom [ rdf:type owl:Class ;
                                             owl:complementOf :PratoComPolvo
                                           ]
                       ] .


###  http://example.org/polvo-filosofico#Prato
:Prato rdf:type owl:Class .


###  http://example.org/polvo-filosofico#PratoCarnivoro
:PratoCarnivoro rdf:type owl:Class ;
                rdfs:subClassOf :Prato ;
                owl:disjointWith :PratoVegano .


###  http://example.org/polvo-filosofico#PratoComPolvo
:PratoComPolvo rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :temIngrediente ;
                                     owl:someValuesFrom :Polvo
                                   ] ;
               rdfs:subClassOf :Prato .


###  http://example.org/polvo-filosofico#PratoOntologicamenteAmbiguo
:PratoOntologicamenteAmbiguo rdf:type owl:Class ;
                             rdfs:subClassOf :Prato .


###  http://example.org/polvo-filosofico#PratoVegano
:PratoVegano rdf:type owl:Class ;
             rdfs:subClassOf :Prato ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :temIngrediente ;
                               owl:allValuesFrom [ rdf:type owl:Class ;
                                                   owl:complementOf :IngredienteAnimal
                                                 ]
                             ] .


###  http://example.org/polvo-filosofico#Restaurante
:Restaurante rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty [ owl:inverseOf :trabalhaEm
                                              ] ;
                               owl:someValuesFrom :Cozinheiro
                             ] .


###  http://example.org/polvo-filosofico#RoboCozinheiro
:RoboCozinheiro rdf:type owl:Class ;
                rdfs:subClassOf :Cozinheiro ,
                                :Maquina .


